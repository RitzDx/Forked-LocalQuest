<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <InvariantGlobalization>true</InvariantGlobalization>
    <ApplicationIcon>EqIcon(1).ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="EqIcon(1).ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="HttpMultipartParser" Version="8.4.0" />
    <PackageReference Include="SixLabors.ImageSharp" Version="3.1.5" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="QuerryNetworking">
      <HintPath>Shared\QuerryNetworking.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
